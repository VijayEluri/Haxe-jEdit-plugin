<?xml version="1.0"?>
<!DOCTYPE ACTIONS SYSTEM "actions.dtd">
<ACTIONS>
    <ACTION NAME="haxe-build-project">
        <CODE>
                        sidekick.haxe.HaXeSideKickPlugin.buildProject();
        </CODE>
    </ACTION>
    <ACTION NAME="haxe-launch-project">
        <CODE>
                        sidekick.haxe.HaXeSideKickPlugin.launchProject();
		</CODE>
    </ACTION>
    <ACTION NAME="haxe-code-completion">
        <CODE>
                        sidekick.haxe.HaXeSideKickPlugin.haxeCodeComplete(view);
		</CODE>
    </ACTION>
    <ACTION NAME="haxe-add-missing-imports">
        <CODE>
                        sidekick.haxe.ImportManager.addMissingImports(view);
		</CODE>
    </ACTION>
    <ACTION NAME="haxe-add-import-at-caret">
        <CODE>
                        sidekick.haxe.ImportManager.addImport(view);
		</CODE>
    </ACTION>
    <ACTION NAME="haxe-add-using-at-caret">
        <CODE>
                        sidekick.haxe.ImportManager.addImport(view, true);
		</CODE>
    </ACTION>
    <ACTION NAME="haxe-go-to-last-edit-location">
        <CODE>
                        sidekick.haxe.HaXeSideKickPlugin.goToLastEditLocation(view);
		</CODE>
    </ACTION>
    <ACTION NAME="haxe-clear-import-cache">
        <CODE>
                        sidekick.haxe.ImportManager.clearImportCache();
		</CODE>
    </ACTION>
</ACTIONS>